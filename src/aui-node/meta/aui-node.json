{
    "aui-node": {
        "use": [
            "aui-node-base"
        ]
    },

    "aui-node-base": {
        "requires": [
            "node-base",
            "node-style",
            "node-pluginhost",
            "node-screen",
            "array-extras",
            "aui-base-lang",
            "aui-classnamemanager",
            "aui-debounce"
        ]
    },

    "aui-node-html5": {
        "condition": {
            "name": "aui-node-html5",
            "trigger": "aui-node",
            "ua": "ie"
        },

        "requires": [
            "collection",
            "aui-node-base"
        ]
    }
}
