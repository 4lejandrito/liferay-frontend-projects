{
	"dependencies": {
		"gulp": "^3.9.1",
		"replace-in-file": "^3.4.2"
	},
	"devDependencies": {
		"chai": "4.2.0",
		"chai-fs": "2.0.0",
		"jest": "^24.1.0",
		"sinon": "9.2.1"
	},
	"jest": {
		"modulePathIgnorePatterns": [
			"/generator-liferay-theme/generators/layout/"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/__fixtures__/"
		]
	},
	"keywords": [
		"yeoman-generator"
	],
	"private": true,
	"resolutions": {
		"ansi-colors": "3.2.4"
	},
	"scripts": {
		"changelog": "npx liferay-changelog-generator",
		"changelog:regenerate": "npx liferay-changelog-generator --regenerate",
		"ci": "yarn format:check && yarn lint && yarn test",
		"format": "cd ../../.. && yarn format",
		"format:check": "cd ../../.. && yarn format:check",
		"lint": "cd ../../.. && yarn lint",
		"lint:fix": "cd ../../.. && yarn lint:fix",
		"test": "jest",
		"updatePackageVersions": "replace-in-file --configFile=update-package-versions-config.js"
	},
	"workspaces": [
		"packages/*"
	]
}
