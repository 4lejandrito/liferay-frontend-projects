{
	"bin": {
		"liferay": "./liferay.js"
	},
	"dependencies": {
		"@liferay/portal-adapt-base": "^1.0.0",
		"liferay-npm-bundler": "*"
	},
	"description": "Autopreset for adapting @angular/cli projects",
	"main": "config.json",
	"name": "@liferay/portal-adapt-angular-cli",
	"scripts": {
		"build": "true",
		"ci": "cd ../.. && yarn ci",
		"format": "cd ../.. && yarn format",
		"format:check": "cd ../.. && yarn format:check",
		"lint": "cd ../.. && yarn lint",
		"lint:fix": "cd ../.. && yarn lint:fix",
		"postversion": "npx liferay-js-publish",
		"preversion": "yarn ci",
		"test": "cd ../.. && yarn test"
	},
	"version": "1.0.0"
}
